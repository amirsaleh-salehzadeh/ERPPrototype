{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "BFF.Gateway.Middleware.RequestLoggingMiddleware": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    }
  },
  "Elasticsearch": {
    "Uri": "http://localhost:9200",
    "IndexFormat": "bff-gateway-dev-logs-{0:yyyy.MM.dd}"
  }
}
